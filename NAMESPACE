## Import of packages
import(
	foreign,
	methods,
	plotKML,
	sp,
	taxlist,
	vegdata
)

## Import of functions
importFrom("stats", "aggregate", "na.omit", "terms")
importFrom("utils", "write.table")

## Export of regular function names
export(
	import_coverconvert,
	import_vegtable,
	subset_by_head,
	subset_by_popup,
	subset_by_species,
	vegtable_stat
)

## Export of classes
exportClasses(
	vegtable
)

## Export of methods
exportMethods(
	"[", "[<-", "$", "$<-",
	crosstable,
	head,"head<-",
	obj2kml,
	popup,"popup<-",
	summary,
	write_juice
)
