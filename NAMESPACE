## Import of packages
import(
	foreign,
	methods,
	taxlist,
	vegdata
)

## Export of regular function names
export(
	import_coverconvert,
	import_vegtable,
	subset_by_head,
	subset_by_popup,
	subset_by_species,
	vegtable_stat
)

## Export of classes
exportClasses(
	vegtable
)

## Export of methods
exportMethods(
	merge_species,
	popup,
	"popup<-",
	summary
)
