## Import of packages
import(
	foreign,
	methods,
	plotKML,
	sp,
	taxlist,
	vegdata
)

## Import of functions
importFrom("stats", "aggregate", "na.omit", "terms")
importFrom("utils", "write.table")


## Export of classes
exportClasses(
	vegtable
)

## Export of methods
exportMethods(
	"[", "[<-", "$", "$<-",
	crosstable,
	df2vegtable,
	header,"header<-",
	obj2kml,
	summary,
	veg_relation,"veg_relation<-",
	write_juice
)

## Export of regular function names
export(
	import_coverconvert,
	subset_by_header,
	subset_by_popup,
	subset_by_species,
	tv2vegtable,
	vegtable_stat
)
