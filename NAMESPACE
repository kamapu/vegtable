## Import of packages
import(
	foreign,
	methods,
	plotKML,
	sp,
	taxlist,
	vegdata
)

## Import of functions
importFrom("stats", "aggregate", "na.omit", "terms")
importFrom("utils", "object.size", "write.table")
importFrom("taxlist", "S4_to_list", "as.list")

## Export of classes
exportClasses(
	coverconvert,
	vegtable
)

## Export of methods
exportMethods(
	"[", "[<-", "$", "$<-",
	clean,
	crosstable,
	df2vegtable,
	dimnames,
	names,
	header,"header<-",
	vegtable2kml,
	subset,
	summary,
	transform,
	veg_relation,"veg_relation<-",
	write_juice
)

## Export of regular function names
export(
	tv2coverconvert,
	tv2vegtable,
	vegtable_stat
)
